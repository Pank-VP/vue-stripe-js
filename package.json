{
  "name": "vue-stripe-js",
  "version": "1.0.0",
  "description": "Vue 3 components for Stripe.js: Elements & Checkout",
  "main": "./dist/vue-stripe.umd.js",
  "module": "./dist/vue-stripe.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-stripe.es.js",
      "require": "./dist/vue-stripe.umd.js"
    }
  },
  "types": "./types/vue-stripe.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ectoflow/vue-stripe-js.git"
  },
  "keywords": [
    "Vue",
    "Stripe",
    "Elements",
    "Checkout",
    "Payment",
    "Components"
  ],
  "author": "Oleksandr Bystrikov",
  "license": "MIT",
  "homepage": "https://github.com/ectoflow/vue-stripe-js",
  "bugs": {
    "url": "https://github.com/ectoflow/vue-stripe-js/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@stripe/stripe-js": "^1.21.1",
    "@types/node": "^16.11.6",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.3.1",
    "prettier": "^2.3.0",
    "typescript": "^4.1.3",
    "vite": "^2.2.4",
    "vue": "^3.2.21"
  }
}
